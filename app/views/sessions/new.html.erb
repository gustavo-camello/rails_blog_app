<h1>From login</h1>

<%= form_with(scope: :session, url: login_path) do |f| %>
  <p>
    <%= f.label :username %><br>
    <%= f.text_field :username, placeholder: "Enter the username" %>
  </p>

  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.submit "Log in" %>
  </p>

<% end %>
